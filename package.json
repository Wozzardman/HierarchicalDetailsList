{
  "name": "powerapps-filtered-detailslist",
  "version": "17.5.0",
  "description": "Enterprise-grade PowerApps Component Framework (PCF) control with advanced filtering, virtualization, and industry-leading performance.",
  "author": "Josh VanTimmeren",
  "keywords": [
    "pcf",
    "powerapps",
    "datagrid",
    "filtering",
    "virtualization",
    "enterprise",
    "accessibility",
    "performance"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wozzardman/FilteredDetailsListV3.git"
  },
  "scripts": {
    "build": "pcf-scripts build",
    "build:with-cleanup": "pcf-scripts build && node scripts/remove-telemetry.js && node scripts/optimize-for-canvas.js",
    "build:original": "pcf-scripts build",
    "build:minimal": "pcf-scripts build && node scripts/remove-telemetry.js && node scripts/optimize-for-canvas.js && node scripts/create-minimal.js",
    "clean": "pcf-scripts clean",
    "rebuild": "pcf-scripts rebuild",
    "start": "pcf-scripts start",
    "refreshTypes": "pcf-scripts refreshTypes",
    "lint": "eslint DetailsList --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --collectCoverage --maxWorkers=4",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:performance": "jest --config=jest.performance.config.js",
    "test:accessibility": "axe-core --include=DetailsList",
    "test:visual": "backstop test",
    "analyze": "npm run build && webpack-bundle-analyzer dist/bundle.js",
    "security": "npm audit && snyk test",
    "docs": "typedoc --out docs DetailsList",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@fluentui/react": "8.121.1",
    "@tanstack/react-virtual": "^3.13.12",
    "comlink": "^4.4.2",
    "date-fns": "^2.30.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "immer": "^10.1.1",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.5.31",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "rxjs": "^7.8.2",
    "xlsx": "^0.18.5",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@fluentui/jest-serializer-merge-styles": "^8.0.17",
    "@playwright/test": "^1.40.0",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/aria-query": "^5.0.4",
    "@types/conventional-commits-parser": "^5.0.1",
    "@types/enzyme": "^3.10.11",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/exceljs": "^0.5.3",
    "@types/file-saver": "^2.0.7",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^27.4.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^18.8.2",
    "@types/powerapps-component-framework": "^1.3.4",
    "@types/react": "^16.14.60",
    "@types/react-dom": "^16.9.24",
    "@types/react-test-renderer": "^18.0.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/tough-cookie": "^4.0.5",
    "@types/yauzl": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "axe-core": "^4.8.3",
    "backstopjs": "^6.2.2",
    "commitizen": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "husky": "^8.0.3",
    "jest": "^27.4.7",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "pcf-scripts": "^1",
    "pcf-start": "^1",
    "prettier": "^3.2.5",
    "react-test-renderer": "16.8.6",
    "semantic-release": "^22.0.12",
    "snyk": "^1.1254.0",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.25.4",
    "typescript": "^4.5.4",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitizen"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "overrides": {
    "trim-newlines": "^3.0.1",
    "ip": "^2.0.1",
    "webpack-dev-middleware": "^5.3.4",
    "postcss": "^8.4.31",
    "braces": "^3.0.3",
    "micromatch": "^4.0.8",
    "tmp": "^0.2.5"
  }
}
